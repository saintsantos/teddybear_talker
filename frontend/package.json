{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "add": "^2.0.6",
    "antd": "^2.13.3",
    "autoprefixer": "7.1.2",
    "axios": "^0.16.2",
    "bootstrap": "^3.3.7",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "director": "^1.2.8",
    "dotenv": "4.0.0",
    "fs-extra": "3.0.1",
    "halogen": "^0.2.0",
    "html-webpack-plugin": "5.5.0",
    "mobx": "^3.3.0",
    "mobx-react": "^4.3.3",
    "mobx-react-devtools": "^4.2.15",
    "mobx-react-form": "^1.32.2",
    "mobx-utils": "^3.2.1",
    "moment": "^2.18.1",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "rc-time-picker": "^2.4.1",
    "react": "^15.6.1",
    "react-audio-player": "^0.6.2",
    "react-audio-recorder": "^2.0.0",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^15.6.1",
    "react-dropzone": "^4.1.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.73.1",
    "shortid": "^2.2.8",
    "validatorjs": "^3.13.5",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && mv ./build/ ../server/python/",
    "test": "node scripts/test.js --env=jsdom",
    "clean": "rm -rf ../server/python/build/"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "6.26.0",
    "css-loader": "0.28.4",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "jest": "^21.1.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "react-dev-utils": "^4.0.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
